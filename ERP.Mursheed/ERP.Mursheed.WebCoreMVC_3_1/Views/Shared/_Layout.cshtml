<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <title>Document</title>
    <link rel="stylesheet" href="https://use.fontawesome.com/releases/v5.6.0/css/all.css"
          integrity="sha384-aOkxzJ5uQz7WBObEZcHvV5JvRW3TUc2rNPA7pe3AwnsUohiw1Vj2Rgx2KSOkF5+h" crossorigin="anonymous">
    <link rel="stylesheet" href="~/ui/libs/swiper/package/css/swiper.min.css">
    <link rel="stylesheet" href="~/ui/css/main.min.css">
    <link href="~/select2CSS.css" rel="stylesheet" />
    @RenderSection("styles", false)
    <style>
    </style>
</head>
<body>
    <header class="header">
        <div class="inner-header">
            <div class="container">
                <div class="inside-inner">
                    <a href="">Support</a>
                    <a href="">Contact us</a>
                </div>
            </div>
        </div>
        <div class="center-header">
            <div class="container">
                <div class="wrapper">
                    <div class="logo">
                        <a href="/Home/Index">
                            <img src="~/ui/img/header/logo.svg" alt="">
                        </a>
                    </div>
                    <div class="right-side-header">
                        <div class="currency-country">
                            <div class="currency">
                                <span>Currency</span>
                                <!-- <p>AZN</p> -->
                                <div class="custom-selectbox">
                                    <select>
                                        <option value="0">AZN</option>
                                        <option value="1">Dollar</option>
                                        <option value="2">Euro</option>
                                        <option value="3">TL</option>
                                        <option value="4">Rubl</option>
                                    </select>
                                </div>
                            </div>
                            <div class="country">
                                <span>Country</span>
                                <div class="custom-selectbox">
                                    <select>
                                        <option value="0">Azerbaijan</option>
                                        <option value="1">Turkey</option>
                                        <option value="2">Germany</option>
                                        <option value="3">France</option>
                                    </select>
                                </div>
                            </div>

                        </div>
                        <partial name="_LoginRegister"/>
                    </div>
                </div>
            </div>

        </div>
        <div class="menu">
            <div class="container">
                <nav>
                    <ul>
                        <li class="active"><a href="index.html">Home</a></li>
                        <li><a href="about.html">About</a></li>
                        <li><a href="guides_drivers.html">Guides & Drivers</a></li>
                        <li><a href="offers.html">Offers</a></li>
                    </ul>
                </nav>
            </div>
        </div>
    </header>
    @RenderBody()
    <footer>
        <div class="container">
            <div class="wrapper">
                <div class="row">
                    <div class="col-md-3 col-sm-6">
                        <div class="social">
                            <div class="logo">
                                <a href="index.html"><img src="~/ui/img/footer/logo-dark.svg" alt=""></a>
                            </div>
                            <div class="description">
                                <p>Mursheed helps you to have an unforgettable experience anywhere you travel to. </p>
                            </div>
                            <div class="follow-us">
                                <p class="header-text">Follow us</p>
                                <ul>
                                    <li><a href=""><img src="~/ui/img/footer/facebook-letter-logo.svg" alt=""></a></li>
                                    <li><a href=""><img src="~/ui/img/footer/instagram.svg" alt=""></a></li>
                                    <li><a href=""><img src="~/ui/img/footer/twitter-logo-silhouette.svg" alt=""></a></li>
                                    <li><a href=""><img src="~/ui/img/footer/linkedin-letters.svg" alt=""></a></li>
                                </ul>
                            </div>
                        </div>
                    </div>
                    <div class="col-md-3 col-sm-6">
                        <div class="site-map">
                            <p class="header-text">Site map</p>
                            <ul>
                                <li><a href="">Home</a></li>
                                <li><a href="about.html">About</a></li>
                                <li><a href="">Guides & Drivers</a></li>
                                <li><a href="offers.html">Offers</a></li>
                                <li><a href="">Support</a></li>
                            </ul>
                        </div>
                    </div>
                    <div class="col-md-3 col-sm-6">
                        <div class="more">
                            <p class="header-text">More</p>
                            <ul>
                                <li><a href="">Terms & conditions</a></li>
                                <li><a href="">Privacy policy</a></li>
                                <li><a href="">Cookies</a></li>
                            </ul>
                        </div>
                    </div>
                    <div class="col-md-3 col-sm-6">
                        <div class="contact-subscribe">
                            <div class="contact">
                                <p class="header-text">Contact us</p>
                                <ul>
                                    <li><a href="">info@mursheed.com</a></li>
                                    <li><a href="">+994 50 808 08 01</a></li>
                                </ul>
                            </div>
                            <div class="subscribe">
                                <p class="header-text">Subscribe</p>
                                <div class="send">
                                    <input placeholder="your email address" type="text">
                                    <button><img src="~/ui/img/footer/send.svg" alt=""></button>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </footer>
    <script src="~/jquery/jquery.min.js"></script>
    <script src="~/ui/libs/swiper/package/js/swiper.min.js"></script>
    <script src="~/ui/js/swiper.js"></script>
    <script src="~/ui/js/swiper_most_popular_drivers.js"></script>
    <script src="~/ui/js/swiper_popular_attractions_locations.js"></script>
    <script src="~/ui/js/swiper_our_offers_partners.js"></script>
    <script src="~/ui/js/swiper_latest_tourists_reviews.js"></script>
    <script src="~/ui/js/swiper_crated_for.js"></script>
    <script src="~/ui/js/selectbox.js"></script>
    @RenderSection("scripts", false)
</body>

</html>